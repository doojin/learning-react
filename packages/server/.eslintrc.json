{
    "extends": [
        "standard"
    ],
    "plugins": [
        "jest"
    ],
    "env": {
        "jest/globals": true
    },
    "overrides": [
        {
            "files": [
                "*.test.js"
            ],
            "rules": {
                "import/first": "off"
            }
        }
    ]
}